<div class="popup_wrap" id={{ title }}>
	<div class="inner">
		<div class="popup_head">
			<h1>{{ item.title }}</h1>
		</div>
		<div class="popup_cont">
			<ul>
				{% for desc_text in item.desc %}
					<li>{{ desc_text }}</li>
				{% endfor %}
			</ul>
		</div>
		<button type="button" class="popup_close">
			<i class="ic_close"><span class="blind">팝업 닫기</span></i>
		</button>
	</div>
</div>
